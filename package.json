  "license": "GPL-3.0",
  "engines": {
    "node": ">=11.0.0",
    "yarn": "^1.13.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "analyze": "yarn build --analyze",
    "build": "yarn get-imported && babel-node tools/run build --release",
    "dev": "yarn get-imported && babel-node tools/run start",
    "dev-docker": "docker-compose up",
    "docker-install": "docker-compose -f docker-compose.builder.yml run install",
    "get-imported": "imported-components src ssr/client/imported.js",
    "git-up": "pm2 stop $npm_package_name && yarn git-pull && pm2 start $npm_package_name && yarn render",
    "git-pull": "git pull && yarn install && yarn build",
    "git-watch": "pm2 stop $npm_package_name && yarn git-pull && yarn start",
    "plop": "plop",
    "prod": "yarn build && yarn start",
    "render": "babel-node tools/render",
    "serve": "yarn build && babel-node build/server",
    "start": "node build/server",
    "pm2": "yarn build && pm2 start build/server.js --name $npm_package_name",
    "pm2:restart": "yarn build && pm2 restart $npm_package_name",
    "pm2:stop": "pm2 stop $npm_package_name"
  },
  "moduleRoots": [
    "ssr/client",
    "ssr/server",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
