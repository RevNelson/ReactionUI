  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.11.3",
    "yarn": "^1.12.3"
  },
  "scripts": {
    "analyze": "yarn-or-npm build:with-stats && yarn-or-npm start:analyzer",
    "build": "yarn-or-npm generate-imported-component && cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "generate-imported-component": "imported-components src src/client/imported.js",
    "plop": "plop",
    "start": "yarn-or-npm build && node build/server/server.js",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "dev": "cross-env NODE_ENV=development yarn-or-npm generate-imported-component && node scripts/start.js",
    "git-pull": "git pull && yarn-or-npm install",
    "git-up": "pm2 stop $npm_package_name && yarn-or-npm git-pull && yarn-or-npm build && pm2 start $npm_package_name",
    "git-watch": "pm2 stop $npm_package_name && yarn-or-npm git-pull && yarn-or-npm start",
    "pm2": "yarn-or-npm build && pm2 start build/server/server.js --name $npm_package_name",
    "pm2:restart": "yarn-or-npm build && pm2 restart $npm_package_name",
    "pm2:stop": "pm2 stop $npm_package_name"
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
