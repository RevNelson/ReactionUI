{
  "name": "reactionui",
  "version": "1.0.0",
  "repository": "git@github.com:RevNelson/reactionui.git",
  "author": "Michael Nelson <revnelson@gmail.com>",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.11.3",
    "yarn": "^1.12.3"
  },
  "browserslist": [
    "last 4 versions",
    "ie >= 9"
  ],
  "scripts": {
    "build": "rimraf dist && yarn generate-imported-components && yarn create-bundles",
    "copy-lang": "cp -R ./src/shared/i18n/locales ./build/client/locales",
    "create-bundles": "concurrently 'yarn create-bundle:client' 'yarn create-bundle:server' && yarn copy-lang",
    "create-bundle:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d build/client --cache-dir build/cache/client --detailed-report",
    "create-bundle:server": "cross-env BABEL_ENV=server parcel build src/server/index.js -d build/server --target node --cache-dir build/cache/server",
    "dev": "nodemon -e js,jsx,html --ignore build --ignore src/client/imported.js --exec 'yarn build && yarn start'",
    "dev-client": "npm run generate-imported-components && parcel src/client/index.html --hmr-port 3335",
    "dev-docker": "docker-compose up",
    "docker-install": "docker-compose -f docker-compose.builder.yml run install",
    "generate-imported-components": "imported-components src src/client/imported.js",
    "git-up": "pm2 stop $npm_package_name && yarn git-pull && pm2 start $npm_package_name",
    "git-pull": "git pull && yarn install && yarn build",
    "git-watch": "pm2 stop $npm_package_name && yarn git-pull && yarn start",
    "plop": "plop",
    "prod": "yarn build && yarn start",
    "start": "node build/server/index.js",
    "pm2": "yarn build && pm2 start build/server/index.js --name $npm_package_name",
    "pm2:restart": "yarn build && pm2 restart $npm_package_name",
    "pm2:stop": "pm2 stop $npm_package_name"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "antd": "^3.13.6",
    "apollo-cache-inmemory": "^1.5.0",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.5.0",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "components": "^0.1.0",
    "compression": "^1.7.3",
    "cookie": "^0.3.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "i18next": "^15.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-chained-backend": "^1.0.1",
    "i18next-express-middleware": "^1.7.1",
    "i18next-localstorage-backend": "^2.1.2",
    "i18next-node-fs-backend": "^2.1.1",
    "i18next-xhr-backend": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.0",
    "react": "^16.8.3",
    "react-apollo": "^2.5.0",
    "react-dom": "^16.8.3",
    "react-helmet-async": "^0.2.0",
    "react-i18next": "^10.2.0",
    "react-imported-component": "^5.5.3",
    "react-router-dom": "^4.3.1",
    "react-svg-inline": "^2.1.1",
    "recompose": "^0.30.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@svgr/parcel-plugin-svgr": "^4.1.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "i18next-parser": "^0.13.0",
    "less": "2.7.3",
    "llog": "^0.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-compress": "^1.0.7",
    "plop": "^2.2.0",
    "puppeteer": "^1.12.1",
    "react-hot-loader": "^4.7.1",
    "rimraf": "^2.6.3"
  }
}
